{
  "expo": {
    "name": "b3-tracker",
    "slug": "b3-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/b3-tracker-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ceub.b3-tracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/b3-tracker-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ceub.b3_tracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/b3-tracker-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "@prisma/react-native"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6991bd7b-6929-4085-9d9e-d5338c3ac95d"
      }
    }
  }
}